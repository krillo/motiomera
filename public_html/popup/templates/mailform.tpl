<h1>Kontakta oss</h1>
<form action="" method="post" onsubmit="if(mm_kontaktValidera(this)) motiomera_skickaMail(); return false;">

	<table border="0" cellpadding="0" cellspacing="0" class="motiomera_form_table nolines">
		<tr>
			<th>E-postadress</th>
			<td><input type="text" name="epost" id="kontaktEpost" /></td>
		</tr>
		<tr>
			<th>Meddelande</th>
			<td>
				<textarea name="meddelande" id="kontaktMeddelande"></textarea>
			</td>
		</tr>
		<tr class="mmLastRow">
			<td></td>
			<td><input type="submit" value="Skicka" /></td>
		</tr>
	</table>
</form>